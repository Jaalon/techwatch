<service>
  <id>TechWatch</id>
  <name>TechWatch</name>
  <description>TechWatch Service</description>
  <executable>%BASE%\techwatch.exe</executable>
  <logpath>%BASE%\logs</logpath>
  <log mode="roll-by-size" sizeThreshold="10485760" keepFiles="7"/>
  <onfailure action="restart" delay="10 sec"/>
  <stoptimeout>30 sec</stoptimeout>
</service>
